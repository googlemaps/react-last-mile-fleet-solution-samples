    "tslint-react": "^5.0.0",
    "webpack-dev-server": "~3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/google.maps": "^3.49.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
