    "@types/jest": "^27.5.1",
    "@types/react-test-renderer": "^18.0.0",
    "expo": "~45.0.0",
    "expo-status-bar": "~1.3.0",
    "jest": "^28.1.0",
    "jest-expo": "^45.0.1",
    "react": "^18.1.0",
    "react-dom": "17.0.2",
    "react-native": "^0.68.2",
